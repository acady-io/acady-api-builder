{
  "name": "acady-api-builder",
  "version": "0.0.1-dev",
  "private": true,
  "description": "Acady API Builder",
  "main": "./build/index.js",
  "files": [
    "build"
  ],
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc",
    "prepare": "npm run build",
    "test": "npm run prepare && jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "license": "MIT",
  "author": "schab",
  "repository": {
    "type": "git",
    "url": "https://github.com/acady-io/acady-api-builder.git"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "release": {
    "repositoryUrl": "https://github.com/acady-io/acady-api-builder.git",
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  }
}
